{
  "compilerOptions": {
    "target": "es2017",
    "module": "esnext",
    "lib": ["esnext"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "paths": {
      "~/*": ["./src/*"],
      "unstorage/drivers/*": ["./node_modules/unstorage/dist/drivers/*.d.ts"]
    }
  },
  "include": [
    "./src/*",
    "./routes/*",
    "./env.d.ts"
  ],
  "extends": "./.nitro/types/tsconfig.json"
}